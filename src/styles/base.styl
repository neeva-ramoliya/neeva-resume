
html {
  font-size: 16px;
}
@media (max-width: 900px) {
  html { font-size: 16px; }
}
@media (max-width: 600px) {
  html { font-size: 12px; }
}
@media (max-width: 400px) {
  html { font-size: 10px; }
}

.container
    width: 100%
    .text-jenthill
        font-family: 'Jenthill', sans-serif;
    .text-mosk
        font-family: 'Mosk', sans-serif;
    .theme-color-text
      color: #a2d1d2
    .desc-color-text
      color: black
    .margin-right-10
      margin-right: 10px
    .content
      background: white
      width: 100%
    .footer
      background-image: url('/assets/footer.png')
      background-size: 100% 350px
      background-position: center center
      width: 100%
      height: 350px
      background-repeat: no-repeat, repeat
            
